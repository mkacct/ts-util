	},
	"scripts": {
		"prebuild": "rimraf build",
		"build": "npx tsc",
		"test": "node --test \"build/test/**/!(*.sub).test.js\""
	},
	"devDependencies": {
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.5.1",
		"eslint": "^9.9.1",
		"rimraf": "^6.0.1",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.3.0"
