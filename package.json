{
	"name": "@mkacct/ts-util",
	"version": "4.0.1",
	"author": {"name": "MK", "url": "https://mkps.app"},
	"description": "Utilities convenient for any JS/TS project",
	"keywords": ["util"],
	"repository": {"type": "git", "url": "git+https://github.com/mkacct/ts-util.git"},
	"bugs": {"url": "https://github.com/mkacct/ts-util/issues"},
	"license": "MIT",
	"type": "module",
	"files": ["build/main"],
	"exports": {
